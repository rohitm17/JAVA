import java.util.*;
class Fibonacci
{
	public static void main(String args[])
	{int i,n;
	Scanner s=new Scanner(System.in);
	System.out.println("Enter no of Digits you want to print");
	n=s.nextInt();
	int n1=0;
	System.out.print(""+n1);
	int n2=1;
	System.out.print(" "+n2);
	while(n>0)
	{ i=n1+n2;
		System.out.print(" "+i);
		n1=n2;
		n2=i;
		n=n-1;
	}
	
	

	}
}
