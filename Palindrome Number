import java.util.*;
class Palindrome
{
	public static void main(String args[])
	{
		int p,n,m,sum=0;
		Scanner s=new Scanner(System.in);
		System.out.println("ENTER NO");
		n=s.nextInt();
		m=n;
		while(n!=0)
		{
			p=n%10;
			sum=sum*10+p;
			n=n/10;
			
		}
		if(m==sum)
		{
			System.out.println("NO IS Palindrome");

		}
		else
		{
			System.out.println("NOT Palindrome");
		}


	}
}
